# https://beebom.com/how-hide-desktop-icons-mac/
function hideDesktopIcons() {
	defaults write com.apple.finder CreateDesktop false;
	killall Finder;
}

function showDesktopIcons() {
	defaults write com.apple.finder CreateDesktop true;
	killall Finder;
}


